{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contributors": [
    "This project is made possible with Patreon support from players like you. Thank you!",
    "${mod_supporters}"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_source}",
    "issues": "${mod_issues}"
  },
  "icon": "logo.png",
  "license": "${mod_license}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.darkhax.botanypots.BotanyPotsFabric"
    ],
    "client": [
      "net.darkhax.botanypots.BotanyPotsFabricClient"
    ],
    "jei_mod_plugin": [
      "net.darkhax.botanypots.addons.jei.JEIPlugin"
    ],
    "rei_client": [
      "net.darkhax.botanypots.addons.rei.REIPlugin"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric": "*",
    "minecraft": "1.19.4",
    "java": ">=17",
    "bookshelf": ">=18"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.twitter": "${mod_twitter}",
        "modmenu.discord": "${mod_discord}",
        "modmenu.curseforge": "${mod_homepage}",
        "modmenu.patreon": "${patreon.campaignUrlTracked}"
      }
    }
  }
}
